{
	"name": "misskey-js",
	"version": "0.0.15",
	"description": "Misskey SDK for JavaScript",
	"main": "./built/index.js",
	"types": "./built/index.d.ts",
	"scripts": {
		"build": "tsc",
		"tsc": "tsc",
		"tsd": "tsd",
		"api": "pnpm api-extractor run --local --verbose",
