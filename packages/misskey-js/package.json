		"@typescript-eslint/parser": "5.8.1",
		"eslint": "8.6.0",
		"jest": "^29.5.0",
		"jest-fetch-mock": "^3.0.3",
		"jest-websocket-mock": "^2.2.1",
		"mock-socket": "^9.0.8",
		"ts-jest": "^29.0.5",
		"ts-node": "10.4.0",
		"tsd": "^0.19.1",
		"typescript": "4.5.4"
